{
    "name": "php/web-php",
    "description": "The www.php.net site.",
    "license": "proprietary",
    "type": "project",
    "homepage": "https://www.php.net/",
    "support": {
        "source": "https://github.com/php/web-php"
    },
    "require": {
        "php": "~8.2.0"
    },
    "require-dev": {
        "ext-curl": "*",
        "friendsofphp/php-cs-fixer": "^3.59.3",
        "phpunit/phpunit": "^11.2.5"
    },
    "autoload": {
        "psr-4": {
            "phpweb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "phpweb\\Test\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "8.2.0"
        },
        "sort-packages": true
    }
}
